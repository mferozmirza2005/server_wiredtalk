{
  "version": 2,
  "builds": [
    {
      "src": "./index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["tsconfig.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": ["http://47.128.231.167", "https://wiredtalk-backend.vercel.app/"]
      }
    }
  ]
}
